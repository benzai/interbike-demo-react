:@template"">/* ----- Variables ----- */

$button-color: 				#45b597; /* green */
$button-color-hover: 		#394552;
$alternate-button-color: 	#e95354; /* pink */
$button-text-color: 		#fcfcfc; 
$beauty-text-color: 		#00ace9; /* blue */
$base-text-color: 			#42505e; /* dark grey */
$sub-text-color: 			#546678; /* light grey */
$on-dark-text-color: 		#8d99a6;
$on-dark-text-color-hover: 	#a3b0bf;

$bg-color: 					#f7f7f7;
$bar-color: 				#394552; /* dark grey */
$separator-color: 			#d9d9d9;

@mixin font-family-body {
	font-family: ProximaNova-Regular, Arial;
}

@mixin font-family-heading {
	font-family: GothamHTF-Light;
}

@mixin font-family-span {
	font-family: GothamHTF-Book;
}

@mixin border-radius-button {
	-webkit-border-radius: 3px;
	-moz-border-radius: 3px;
	border-radius: 3px;
}

@mixin border-radius-round {
	-webkit-border-radius: 500px;
	-moz-border-radius: 500px;
	border-radius: 500px;
}

@mixin sprite-main($rep, $pos) {
	// background: url(data:image/svg+xml;base64,) $rep $pos;
	background: url(../img/sprites.svg) $rep $pos;
}

@mixin center-div-absolute($offset, $width, $height) {
	margin-left: $offset;
	position: absolute;
	bottom: 0;
	left: 50%;
	width: $width;
	height: $height;
}

/* ----- Base ----- */

/* fonts */

@font-face {
	font-family: 'GothamHTF-Light';
	src: url('../fonts/GothamHTF-Light.otf');
}

@font-face {
	font-family: 'GothamHTF-Book';
	src: url('../fonts/GothamHTF-Book.otf');
}

@font-face {
	font-family: 'ProximaNova-Regular';
	src: url('../fonts/ProximaNova-Regular.otf');
}

/* base elements */

body {
	margin: 0;
	padding: 0;
	background-color: $bg-color;
	color: $base-text-color;
	@include font-family-body;
	font-size: 16px;
}

section .container {
	position: relative;
}

h1,
h2,
h3 {
	margin: 0;
	padding: 0;
	text-align: center;
	@include font-family-heading;
	font-weight: normal;
	font-style: normal;
	line-height: 1em;
	text-rendering: optimizelegibility;	
}

h1 {
	margin-top: 64px;
	color: $base-text-color;
	font-size: 40px;
}

h2 {
	color: $beauty-text-color;
	font-size: 100px;
	letter-spacing: -3px;
}

h3 {
	color: $base-text-color;
	@include font-family-span;
	font-size: 20px;
}

p {
	margin: 0;
	padding: 0;
	color: $sub-text-color;
	font-size: 20px;
	line-height: 2em;
	text-align: center;
}

span {
	display: block;
	color: $base-text-color;
	@include font-family-span;
	font-size: 20px;
	text-align: center;
}

ul,
ol {
	margin: 0;
	padding: 0;
}

li {
	display: block;
	color: $sub-text-color;
	font-size: 16px;
	text-align: left;

	a {
		display: inline-block;
	}
}

a {
	color: $sub-text-color;
	text-decoration: none;
}
a:hover, a.selected {
	color: $sub-text-color;
	text-decoration: underline;
}

img {
	display: block;
	margin: 0 auto;
	padding: 0;
	width: 100%;
	border: 0;
}

/* background */

.background-a {
	background-color: #f7f7f7;
}

.background-b {
	background-color: #fff;
}

/* normal button */

.button a {
	display: inline-block;
	padding: 12px 20px;
	background-color: $button-color;
	@include font-family-span;
	font-size: 16px;
	color: $button-text-color;
	text-align: center;
	vertical-align: middle;
	@include border-radius-button;
}
.button a:hover {
	background-color: $button-color-hover;
	text-decoration: none;
}

/* full button */

.button.full a {
	display: block;
}

/* next section arrow */

.arrow-next {
	@include center-div-absolute(-20px, 40px, 40px);
	@include sprite-main(no-repeat, -200px -10px);
}

/* ----- Header ----- */

header {
	height: 79px;
	border-bottom: 1px solid $separator-color;
	background-color: #fff;
	opacity: 0.95;
}

/* logo */

#logo {
	margin-top: 12px;
}

#logo a {
	display: block;
	width: 214px;
	height: 56px;
	background: url(../img/sprites.png) no-repeat 0 0;
	text-indent: -9999px;
}
@media only screen and (-webkit-min-device-pixel-ratio: 2) {
     #logo a {
     	background: url(../img/sprites@2x.png) no-repeat 0 0;
     	background-size: 512px;
     }
}

/* nav bar */

.navbar .nav {
	margin: 0;
	margin-top: 22px;
	padding: 0;
}

.navbar .nav > li > a {
	display: inline-block;
	padding: 8px 20px;
	color: $base-text-color;
	@include font-family-span;
	font-size: 16px;
	text-shadow: none;
}

.navbar .nav > li > a:focus,
.navbar .nav > li > a:hover {
	color: $base-text-color;
}

.navbar .nav > .active > a,
.navbar .nav > .active > a:hover,
.navbar .nav > .active > a:focus,
.navbar .nav > .active-temp > a,
.navbar .nav > .active-temp > a:hover,
.navbar .nav > .active-temp > a:focus {
	background-color: $alternate-button-color;
	color: $button-text-color;
	@include border-radius-round;
	transition: opacity 1s;
}

.navbar.scrolling .nav > li.active > a,
.navbar.scrolling .nav > li.active > a:hover,
.navbar.scrolling .nav > li.active > a:focus {
	padding: 8px 20px;
	background-color: transparent;
	color: $base-text-color;
	@include font-family-span;
	font-size: 16px;
}

/* hire me */

header .button {
	float: right;
	margin-top: 18px;
}

/* ----- Footer ----- */

footer {
	background-color: $bar-color;
	height: 240px;

	.row.social {
		margin-top: 120px;
	}

	.row.mainnav {
		margin-top: 20px;
	}
}

/* social */

footer .social a {
	display: block;
	float: left;
	margin-right: 8px;
	width: 44px;
	height: 44px;
	text-indent: -9999px;
}

footer .social a.twitter {
	@include sprite-main(no-repeat, -146px -10px);
}

footer .social a.dribbble {
	@include sprite-main(no-repeat, -146px -64px);
}

/* nav */

footer nav {
	ul {
		margin: 6px 0 0 0;

		li {
			display: inline;

			a {
				display: inline-block;
				margin-right: 32px;
				color: $on-dark-text-color;
				@include font-family-span;
				font-size: 14px;
			}

			a:hover {
				color: $on-dark-text-color-hover;
			}
		}
	}
}

/* copyright */

footer .copyright {
	float: right;
	color: $on-dark-text-color;
	font-size: 16px;
